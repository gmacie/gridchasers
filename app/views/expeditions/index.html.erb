<h1>Grid Expeditions</h1>

<table>
  <thead>
    <tr>
      <th>Grid</th>
      <th>Call</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Qsl info</th>
      <th>More info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expeditions.each do |expedition| %>
      <tr id="<%= dom_id(expedition) %>">
        <td><%= expedition.grid %></td>
        <td><%= expedition.call %></td>
        <td><%= expedition.start_date %></td>
        <td><%= expedition.end_date %></td>
        <td><%= expedition.qsl_info %></td>
        <td><%= expedition.more_info %></td>
      </tr>
      <tr>
        <td><%= link_to 'Show', expedition %></td>
        <td><%= link_to 'Edit', edit_expedition_path(expedition) %></td>
        <td><%= link_to 'Destroy', expedition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <hr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Expedition', new_expedition_path %>
